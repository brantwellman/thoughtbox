<h1>Enter your info and sign up here!</h1>
<h4>or <%= link_to "login", login_path %></h4>

<%= form_for(@user) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Submit" %>
<% end %>
