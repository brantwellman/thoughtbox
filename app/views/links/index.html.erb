
<h1>Submit your new link here!</h1>

<%= form_for(@link) do |f| %>
  <%= f.label :url %>
  <%= f.text_field :url %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.submit "Submit" %>
<% end %>
<br/>
<br/>
<br/>
<br/>
<h1>All of the links belonging to <%= current_user.email %>!</h1>

<% @links.each do |link| %>
  <%= link_to link.url, "#{link.url}" %> -
  <%= link.title %>
  <br/>
<% end %>
